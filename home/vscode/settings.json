{
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "terminal.integrated.defaultProfile.linux": "fish",
  "keyboard.dispatch": "keyCode",
  "files.exclude": {
    "**/.direnv": true
  },
  "[nix]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "[meson]": {
    "editor.formatOnSave": false
  },
  "mesonbuild.downloadLanguageServer": true,
  "nix.enableLanguageServer": true,
  "nix.serverPath": "/var/home/josh/src/nix-analyzer/target/debug/nix-analyzer",
  "godotTools.editorPath.godot4": "/nix/store/hgq3z4f7qzbavgbh0zybmi062prq9ixy-godot4/bin/godot4",
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "rust-analyzer.cargo.buildScripts.enable": true,
  "workbench.sideBar.location": "right",
  "window.customTitleBarVisibility": "auto",
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.trimTrailingWhitespace": true,
  "rust-analyzer.runnables.askBeforeUpdateTest": false,
  "zig.zls.enabled": "on",
  "workbench.editor.empty.hint": "hidden",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  }
}
