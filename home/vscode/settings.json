{
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Catppuccin Macchiato",
  "terminal.integrated.defaultProfile.linux": "fish",
  "keyboard.dispatch": "keyCode",
  "files.exclude": {
    "**/.direnv": true
  },
  "[nix]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "[csharp]": {
    "editor.defaultFormatter": "csharpier.csharpier-vscode"
  },
  "[meson]": {
    "editor.formatOnSave": false
  },
  "mesonbuild.downloadLanguageServer": true,
  "nix.enableLanguageServer": true,
  "nix.serverPath": "/var/home/josh/src/nix-analyzer/target/debug/nix-analyzer",
  "godotTools.editorPath.godot4": "/nix/store/hgq3z4f7qzbavgbh0zybmi062prq9ixy-godot4/bin/godot4",
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "rust-analyzer.cargo.buildScripts.enable": true,
  "workbench.sideBar.location": "right",
  "window.customTitleBarVisibility": "auto",
  "editor.minimap.enabled": false,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.trimTrailingWhitespace": true,
  "rust-analyzer.runnables.askBeforeUpdateTest": false,
  "zig.zls.enabled": "on",
  "workbench.editor.empty.hint": "hidden",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.handleKeys": {
    "<C-d>": false,
    "<C-s>": false,
    "<C-z>": false,
    "<C-p>": false,
    "<C-w>": false
  },
  "editor.lineNumbers": "relative",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.nextInFiles"],
      "silent": true
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prevInFiles"],
      "silent": true
    },
    {
      "before": ["U"],
      "commands": []
    }
  ],
  "python.analysis.typeCheckingMode": "strict",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "[awk]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": [
    "ARGNO",
    "codegen",
    "funct",
    "getline",
    "globl",
    "gsub",
    "riscv",
    "RLENGTH",
    "seqz",
    "sprintf",
    "SUBSEP"
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "chat.instructionsFilesLocations": {
    ".github/instructions": true,
    "/tmp/postman-collections-post-response.instructions.md": true,
    "/tmp/postman-collections-pre-request.instructions.md": true,
    "/tmp/postman-folder-post-response.instructions.md": true,
    "/tmp/postman-folder-pre-request.instructions.md": true,
    "/tmp/postman-http-request-post-response.instructions.md": true,
    "/tmp/postman-http-request-pre-request.instructions.md": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
